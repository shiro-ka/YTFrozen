{
  "manifest_version": 2,
  "name": "YTFrozen",
  "version": "1.0.0",
  "description": "YTFrozen",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background/ytfrozen-main.js",
      "background/ytfrozen-thumbnail-replace.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js",
        "options/apply-options.js",
        "folder/ytfrozen-folder-manager.js",
        "folder/ytfrozen-folder-ui.js",
        "subscriptions/ytfrozen-subs-folder.js",
        "popup/ytfrozen-popup.js",
        "home/ytfrozen-home.js",
        "rate/ytfrozen-rate-manager.js",
        "rate/ytfrozen-rate-ui.js",
        "title/ytfrozen-title.js"
      ],
      "css": [
        "ytfrozen-variables.css",
        "ytfrozen-style.css",
        "popup/ytfrozen-popup.css",
        "home/ytfrozen-home.css",
        "subscriptions/ytfrozen-subs.css",
        "folder/ytfrozen-folder.css",
        "rate/ytfrozen-rate-manager.css"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": {
      "48": "icons/icon48.png"
    },
    "default_title": "YTFrozen"
  },
  "web_accessible_resources": [
    "icons/*"
  ],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}